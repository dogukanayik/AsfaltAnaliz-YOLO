    "%%writefile app.py\n",
    "import streamlit as st\n",
    "from ultralytics import YOLO\n",
    "from PIL import Image\n",
    "import os\n",
    "\n",
    "st.set_page_config(page_title=\"Yol Hasarƒ± Analiz Sistemi\", layout=\"wide\")\n",
    "\n",
    "st.markdown(\"<h1 style='text-align: center;'>üõ£Ô∏è Akƒ±llƒ± Yol Hasarƒ± Tespit Paneli</h1>\", unsafe_allow_html=True)\n",
    "\n",
    "# Modeli Y√ºkle\n",
    "@st.cache_resource\n",
    "def load_model():\n",
    "    
    "    return YOLO(\"best.pt\")\n",
    "\n",
    "model = load_model()\n",
    "\n",
    "# Sidebar Tasarƒ±mƒ±\n",
    "st.sidebar.header(\"üìä Model Ba≈üarƒ±sƒ±\")\n",
    "if os.path.exists(\"confusion_matrix_normalized.png\"):\n",
    "    st.sidebar.image(\"confusion_matrix_normalized.png\", caption=\"Karƒ±≈üƒ±klƒ±k Matrisi\")\n",
    "if os.path.exists(\"results.png\"):\n",
    "    st.sidebar.image(\"results.png\", caption=\"Eƒüitim Sonu√ßlarƒ±\")\n",
    "\n",
    "# Ana Ekran\n",
    "uploaded_file = st.file_uploader(\"Analiz edilecek yol fotoƒürafƒ±nƒ± se√ßin...\", type=['jpg', 'jpeg', 'png'])\n",
    "\n",
    "if uploaded_file is not None:\n",
    "    image = Image.open(uploaded_file)\n",
    "    \n",
    "    col1, col2 = st.columns(2)\n",
    "    \n",
    "    with col1:\n",
    "        st.subheader(\"Orijinal G√∂r√ºnt√º\")\n",
    "        st.image(image, use_container_width=True)\n",
    "        \n",
    "    with col2:\n",
    "        st.subheader(\"YOLOv11L Analiz Sonucu\")\n",
    "        
    "        results = model.predict(image, imgsz=1280)\n",
    "        
    "        res_plotted = results[0].plot()\n",
    "        st.image(res_plotted, use_container_width=True)\n",
    "        \n",
    "        
    "        boxes = results[0].boxes\n",
    "        if len(boxes) > 0:\n",
    "            st.success(f\"Toplam {len(boxes)} adet hasar tespit edildi.\")\n",
    "        else:\n",
    "            st.warning(\"Herhangi bir hasar tespit edilemedi.\")"
